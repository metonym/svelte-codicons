{
  "name": "svelte-codicons",
  "version": "0.18.0",
  "license": "MIT",
  "description": "VS Code Codicons as Svelte components",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "svelte": "./lib/index.js"
    },
    "./lib/*.svelte": {
      "types": "./lib/*.svelte.d.ts",
      "import": "./lib/*.svelte"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:lib": "svelvg glob=@vscode/codicons/src/icons iconIndex",
    "preview": "vite preview",
    "test": "bun test"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "latest",
    "@vscode/codicons": "0.0.42",
    "svelte": "5.43.6",
    "svelvg": "latest",
    "vite": "7.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metonym/svelte-codicons.git"
  },
  "homepage": "https://github.com/metonym/svelte-codicons",
  "bugs": "https://github.com/metonym/svelte-codicons/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "icon",
    "svg",
    "vs code",
    "codicon"
  ],
  "files": [
    "lib"
  ],
  "type": "module"
}
